<?xml version="1.0" ?>
<robot name="landing_gear" xmlns:xacro="http://ros.org/wiki/xacro">
    <!-- <xacro:property name="link1_length" value="0.2"/>
    <xacro:property name="link1_radius" value="0.02"/> -->
<xacro:property name="base_r" value="0.2"/>
<xacro:property name="base_d" value="0.002"/>

<!-- Base Link -->    
<link name="base_link">
    <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
</link>

<joint name="base_link_connection" type="fixed">
    <parent link="base_link"/>
    <child link="base"/>
    <origin xyz="0 0 0" rpy="1.5714 0 0"/>
    <axis xyz="1 0 0"/>
</joint>

<link name="base">
        <visual>
            <origin rpy="0 0 0" xyz="0.01 -0.01 0" />
            <geometry>
                <mesh filename="package://landing_gear_description/meshes/base.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0.01 -0.01 0" />
            <geometry>
                <mesh filename="package://landing_gear_description/meshes/base.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="-0.01 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
</link>

<!-- leg 1 -->
<link name="leg">
    <visual>
        <origin rpy="0 0 0" xyz="0 -0.136 -0.017" />
        <geometry>
            <mesh filename="package://landing_gear_description/meshes/link1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
        <origin rpy="0 0 0" xyz="0 -0.136 -0.017" />
        <geometry>
            <mesh filename="package://landing_gear_description/meshes/link1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0" />
        <mass value="0.1"/>
        <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
</link>


<joint name="base_leg_1_joint" type="revolute">
    <parent link="base"/>
    <child link="leg"/>
    <!-- the link 1 rotated  ni x rad-->
    <origin xyz="0 -0.0058 -0.105" rpy="-1.5714 3.1415 0"/>
    <axis xyz="-1 0 0"/>
    <limit effort="100" velocity="1" lower="-1.5714" upper="1.5714"/>
</joint>


<link name="link2_leg">
    <visual>
        <origin rpy="0 0 0" xyz="0.01 0.0125 -0.0125" />
        <geometry>
            <mesh filename="package://landing_gear_description/meshes/link2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
        <origin rpy="0 0 0" xyz="0.01 0.0125 -0.0125" />
        <geometry>
            <mesh filename="package://landing_gear_description/meshes/link2.stl" scale="0.001 0.001 0.001"/>
