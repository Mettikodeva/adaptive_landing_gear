<!-- IMU  -->

<joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin xyz="0.0 0 0" rpy="0 0 0"/>
  </joint>

<link name="imu_link"/>

<!-- gazebo IMU -->
 <gazebo reference="imu_link">
    <sensor type="imu" name="imu">
      <always_on>true</always_on>
      <visualize>true</visualize>
    </sensor>
    <material>Gazebo/Grey</material>
</gazebo>

 <gazebo reference="imu_link">
    <gravity>true</gravity>
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <topic>__default_topic__</topic>
      <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
        <topicName>imu</topicName>
        <bodyName>imu_link</bodyName>
        <updateRateHZ>10.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu_link</frameName>
        <initialOrientationAsReference>false</initialOrientationAsReference>
      </plugin>
      <pose>0 0 0 0 0 0</pose>
    </sensor>
</gazebo>

<!-- lidar sensor -->
<!-- lidar leg1 -->
<link name="base_scan">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.0001"/>
        <inertia ixx="1" ixy="0.0" ixz="0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
</link>
<joint name="scan_joint" type="fixed">
    <parent link="link2_leg"/>
    <child link="base_scan"/>
    <origin xyz="0 0.05 0" rpy="0 0 1.57"/>
    <axis xyz="1 0 0"/>
</joint>
<gazebo reference="base_scan">
    <gravity>true</gravity>
    <sensor type="ray" name="laser_scan">
        <pose>0 0 0 0 0 0</pose>
        <visualize>true</visualize>
        <always_on>true</always_on>
        <update_rate>5</update_rate>
        <ray>
            <scan>
            <horizontal>
              <samples>1</samples>
              <resolution>1</resolution>    
              
            </horizontal>
            </scan>
            <range>
                <min>0.01</min>
                <max>2</max>
                <resolution>0.015</resolution>
            </range>
            <noise>
                <type>gaussian</type>
                <mean>0.0</mean>
                <stddev>0.0</stddev>
            </noise>
        </ray>
        <plugin name="gazebo_ros_scan_controller" filename="libgazebo_ros_laser.so">
            <robotNamespace>lg</robotNamespace>
            <topicName>lidar1</topicName>
            <frameName>base_scan</frameName>
        </plugin>      
    </sensor>
</gazebo>
