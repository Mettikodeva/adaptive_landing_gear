        <mass value="0.1"/>
        <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
</link>

<joint name="base_link2_leg_1_joint" type="revolute">
    <parent link="leg"/>
    <child link="link2_leg"/>
    <origin xyz="0 -0.12 0" rpy="-1.5714 0 0"/>
    <axis xyz="1 0 0"/>
