<joint name="base_leg_3_joint" type="revolute">
    <parent link="base"/>
    <child link="leg_3"/>
    <!-- the link 1 rotated  ni x rad-->
    <origin xyz="-0.082 0 0.037" rpy="-1.5714 -1.0475 0"/>
    <axis xyz="-1 0 0"/>
    <limit effort="100" velocity="1" lower="-1.5714" upper="1.5714"/>
</joint>

<link name="link2_leg_3">
    <visual>
        <origin rpy="0 0 0" xyz="0.01 0.0125 -0.0125" />
        <geometry>
            <mesh filename="package://landing_gear_description/meshes/link2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
        <origin rpy="0 0 0" xyz="0.01 0.0125 -0.0125" />
        <geometry>
            <mesh filename="package://landing_gear_description/meshes/link2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0" />
        <mass value="0.1"/>
        <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
